  -DANDROID_NDK=/opt/android-sdk/ndk/27.2.12479018 \
  -DCMAKE_ANDROID_NDK=/opt/android-sdk/ndk/27.2.12479018 \
  -DCMAKE_TOOLCHAIN_FILE=/opt/android-sdk/ndk/27.2.12479018/build/cmake/android.toolchain.cmake \
  -DCMAKE_MAKE_PROGRAM=/opt/android-sdk/cmake/3.22.1/bin/ninja \
  -DCMAKE_LIBRARY_OUTPUT_DIRECTORY=/workspace/app/unity/builds/android/unityLibrary/build/intermediates/cxx/Debug/3x3r4i6r/obj/arm64-v8a \
  -DCMAKE_RUNTIME_OUTPUT_DIRECTORY=/workspace/app/unity/builds/android/unityLibrary/build/intermediates/cxx/Debug/3x3r4i6r/obj/arm64-v8a \
  -DCMAKE_BUILD_TYPE=Debug \
